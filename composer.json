{
    "name": "zikula/coremanagermodule-module",
    "description": "A module to handle the core release process",
    "type": "zikula-module",
    "license": "LGPL-3.0+",
    "authors": [
        {
            "name": "Zikula Contributors",
            "homepage": "http://github.com/zikula/CoreManagerMoule"
        }
    ],
    "autoload": {
        "psr-4": { "Zikula\\Module\\CoreManagerModule\\": "" }
    },
    "require": {
        "php": ">5.3.3",
        "knplabs/github-api": "~1.2",
        "vierbergenlars/php-semver": "~3.0",
        "carlosio/jenkins": "dev-master"
    },
    "extra": {
        "zikula": {
            "class": "Zikula\\Module\\CoreManagerModule\\ZikulaCoreManagerModule"
        }
    },
    "config": {
        "vendor-dir": "vendor",
        "preferred-install": "dist",
        "optimize-autoloader": true
    }
}
